# Environment Variables Template
# Copy this to .env.local and fill in with your actual values

# API Configuration
# GitHub API token for fetching repository data
NEXT_PUBLIC_GITHUB_API_TOKEN=

# GitHub Username for repository data
NEXT_PUBLIC_GITHUB_USERNAME=

# Analytics
# Google Analytics ID
NEXT_PUBLIC_GA_ID=

# Site Configuration
# Site URL (e.g., https://example.com)
NEXT_PUBLIC_SITE_URL=

# Contact Email
NEXT_PUBLIC_CONTACT_EMAIL=

# Optional: API rate limiting
API_RATE_LIMIT=60
